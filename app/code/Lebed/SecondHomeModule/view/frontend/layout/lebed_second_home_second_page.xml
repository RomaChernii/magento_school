<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="3columns" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceContainer name="div.sidebar.main">
            <block class="Lebed\SecondHomeModule\Block\Example" name="second.page.example.block" template="Lebed_SecondHomeModule::example.phtml" after="-">
                <arguments>
                    <argument name="css_class" xsi:type="string">page-title</argument>
                </arguments>
            </block>
            <block class="Magento\Framework\View\Element\Template" name="button.popup.lebed" template="Lebed_SecondHomeModule::button.phtml" after="-" />
        </referenceContainer>
    </body>
</page>
